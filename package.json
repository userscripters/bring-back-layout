  "homepage": "https://github.com/userscripters/bring-back-layout#readme",
  "scripts": {
    "test": "echo 'no tests for now'",
    "clean": "rm -rf dist",
    "uglify": "uglifyjs dist/index.user.js --compress --mangle -o dist/index.min.user.js",
